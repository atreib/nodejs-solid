{
  "name": "nodejs-solid",
  "version": "1.0.0",
  "description": "Nodejs Typescript TDD Clean SOLID",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:unit": "jest --passWithNoTests --runInBand -c jest.unit.config.js",
    "test:integrate": "jest --passWithNoTests --runInBand -c jest.integrate.config.js",
    "test:clean": "npm t -- --noStackTrace",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm run test:clean -- --findRelatedTests",
    "test:ci": "npm t -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andev-road/nodejs-solid.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "tdd",
    "clean",
    "solid",
    "api",
    "git",
    "docker",
    "jest"
  ],
  "author": "Andre Treib",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andev-road/nodejs-solid/issues"
  },
  "homepage": "https://github.com/andev-road/nodejs-solid#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "validator": "^13.0.0"
  }
}
